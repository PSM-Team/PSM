html(lang='en')
  head
    meta(charset='UTF-8')
    title Sample Example Upload and store image Using Nodejs and MySQL
    link(rel='stylesheet', href='//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css')
    script(src='https://code.jquery.com/jquery-2.2.0.min.js')

  body
    .row
      .col-xs-12.col-sm-12.col-md-6.col-lg-6.col-xs-offset-0.col-sm-offset-0.col-md-offset-3.col-lg-offset-3.toppad(style='padding-top:30px')
        .panel.panel-info
          .panel-heading
            h3.panel-title
          each dat in data
            .panel-body
              .row
                .col-md-3.col-lg-3(align='center')
                  img.img-circle.img-responsive(alt='User Pic', src='http://localhost:8080/images/upload_images/' + dat.image)
                .col-md-9.col-lg-9
                  table.table.table-user-information
                    tbody
                      tr
                        td User Name:
                        td #{dat.user_name}
